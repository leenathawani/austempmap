{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 700,
    "height": 230,
    "config": {"view": {"stroke": null},  "background": "#fff1de"},
    "data": {
      "url": "https://raw.githubusercontent.com/leenathawani/vegalite_map/main/data/continent_averages.csv"
    },
    "transform": [{
      "sort": [
        {"field": "Avg_LE", "order": "descending"}
      ],
      "window": [{
        "op": "rank",
        "as": "rank"
      }],
      "groupby": ["Year"]
    }],
    "mark": {
        "type": "line",
        "point": true
    },
    "encoding": {
      "x": {"field": "Year", "type": "ordinal"},
      "y": {"field": "rank", "type": "ordinal"},
      "color": {
        "field": "Continent", "type": "nominal",
        "scale": {
          "domain": ["Africa", "Americas", "Asia", "Europe", "Oceania"],
          "range": ["black", "#127153", "#C91A3C", "#0C71AB", "#C91A3C", "#0C71AB"]
        }
      },
      "strokeWidth": {"value": 3}
    }
  }